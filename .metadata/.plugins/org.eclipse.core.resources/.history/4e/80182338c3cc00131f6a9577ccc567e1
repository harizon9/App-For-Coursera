var openWin = null;
var prevWin = null;
var backArray = [];


function navigateApplication(inWindow){
	switch(inWindow.WINDOWTYPE){
		case 'ERROR':
			break;
		case 'MAIN_MENU':
			break;
		case 'REGISTER_TASK':
			break;
		case 'INPUT_TIME':
			break;
		case 'AVAILABLE_TASKS':
			break;
		default:
			break;
	}
}
